#include<stdio.h>
void main()
{
int a,b,c,d,e,f,g;
printf("ENTER 2 NUMBER FOR SUM AND SUBTRACTION");
scanf("%d%d",&a,&b);
c=a+b;
printf("sum =%d",c);
d=a-b;
printf("subtraction=%d",d);
e=a*b;
printf("multiplying=%d",e);


e=a*b;
printf("multiplying=%d",e);
f=a/b;
printf("division=%d",f);
g=a%b;
printf("modulus=%d",g);

}


